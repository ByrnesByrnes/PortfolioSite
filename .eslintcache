 useEffect, useRef } from 'react';\nimport {\n  Container,\n  Item, \n  Link, \n  List\n} from './styles/socials'\n\nlet ClickOutside = handler => {\n  let domNode = useRef()\n\n  useEffect(() => {\n    let checkHandler = event => {\n      if(!domNode.current.contains(event.target)) {\n        handler()\n      }\n    }\n    \n    document.addEventListener('mousedown', checkHandler)\n    return () => document.removeEventListener('mousedown',checkHandler)\n  }, [])\n\n  return domNode\n}\n\n\n\nexport default function Socials({children, setModalShow, ...restProps}) {\n\n  let domNode = ClickOutside(() =>{\n\n    setModalShow(false)\n  }) \n\n  return <Container ref={domNode} {...restProps}>{children}</Container>\n}\n\nSocials.List = function SocialsList({children, ...restProps}) {\n  return <List {...restProps}>{children}</List>\n}\n\nSocials.Item = function SocialsItem({children, ...restProps}) {\n  return <Item {...restProps}>{children}</Item>\n}\n\nSocials.Link = function SocialsLink({children, ...restProps}) {\n  return <Link {...restProps}>{children}</Link>\n}\n","/mnt/c/Users/myron/desktop/developer/portfolio/src/components/socials/styles/socials.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/shapes/index.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/shapes/styles/shapes.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/containers/project.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/project/index.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/project/styles/project.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/projectsData.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/form/index.js",["218","219"],"import React, { useRef, useEffect } from 'react';\nimport { \n  Container,\n  Title,\n  Input,\n  InputBox,\n  TextArea,\n  Frame,\n  Button,\n  Label\n} from './styles/form'\n\nexport default function Form({children, ...restProps