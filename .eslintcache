{ Header } from '../components'\nimport { GiHamburgerMenu } from 'react-icons/gi'\nimport { IoMdExit } from 'react-icons/io'\nimport * as ROUTES from '../constants/routes'\nimport { navLinks } from '../navbarLinks' // data for Links\n\nexport function HeaderContainer() {\n  const [toggle, setToggle] = useState(false)\n  const [theme, setTheme] = useState(false)\n\n  const handleToggle = () => {\n    setToggle(!toggle)\n  }\n\n\n  const handleTheme = () => {\n    console.log('hello')\n  }\n  // 2 sections space them between each other\n  return (\n    <Header>\n        <Header.Logo>\n          <Header.Link exact to={ROUTES.HOME}>MB</Header.Link>\n        </Header.Logo>\n        <Header.Nav toggle={toggle}>\n          <Header.NavList>\n            <Header.Theme onClick={handleTheme} />\n            {navLinks.map(item => (\n              <Header.NavItem key={item.id}>\n                <Header.Link\n                  exact\n                  onClick={() => setToggle(false)}\n                  to={item.link}\n                >{item.title}</Header.Link>\n              </Header.NavItem>\n            ))}\n          </Header.NavList>\n        </Header.Nav>\n        <Header.Toggle onClick={handleToggle} toggle={toggle}>\n          {toggle ? <IoMdExit /> : <GiHamburgerMenu />}\n        </Header.Toggle>\n    </Header>\n  )\n}","/mnt/c/Users/myron/desktop/developer/portfolio/src/components/index.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/header/index.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/header/styles/header.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/globalStyles.js",["170","171"],"import styled, { createGlobalStyle, keyframes } from 'styled-components'\nimport {NavLink as RouterLink } from 'react-router-dom'\nimport { theme } from './containers/header'\n// Colors\nconst primary = '#000'\nconst secondary = '#2196F3'\nconst white = '#fff'\n\n\n// Animations\nexport const revealText = keyframes\n`\n  0% {\n    transform: scaleX(1);\n  }\n  100%{\n    transform: scaleX(0);\n  } \n`\nexport const revealTextReverse = keyframes\n`\n  0% {\n    transform: scaleX(0);\n  }\n  100%{\n    transform: scaleX(1);\n  } \n`\n\nexport const fadeIn = keyframes\n`\n  0% {\n    opacity: 0;\n  }\n  100%{\n    opacity: 1;\n  }\n`\nexport const fadeInBottom = keyframes\n`\n  0% {\n    transform: translateY(100%);\n    opacity: 0;\n  }\n  100% {\n    transform: translateY(0);\n    opacity 1;\n  }\n`\n\nexport const GlobalStyle = createGlobalStyle\n`\n  html { \n    box-sizing: border-box;\n    font-size: 100%;\n  }\n\n  *, *:before, *:after {\n    box-sizing: inherit;\n  }\n\n  :root {\n    --clr-primary: ${primary}; //black\n    --clr-secondary: ${secondary}; //Blue\n    --clr-bg: #fff;\n  }\n\n  body {\n    position: relative;\n    min-height: 100vh;\n    margin: 0;\n    padding: 0;\n    line-height: 1.3;\n    font-family: 'Titillium Web', sans-serif;\n  }\n\n  // ==== General Styles =====\n  h1,h2,h3,h4,h5 {\n    padding: 0;\n    margin: 0;\n  }\n\n  ul {\n    padding: 0;\n    margin: 0;\n    list-style: none;\n  }\n  a {\n    text-decoration: none;\n  }\n`\n\nexport const Link = styled(RouterLink)\n`\n  position: relative;\n  z-index: 20;\n  opacity: 0;\n  font-weight: 600;\n  display: inline-block;\n  margin: 20px 0 0;\n  padding: 10px 20px;\n  background: #000;\n  color: #fff;\n  text-decoration: none;\n  font-size: 1.2em;\n  letter-spacing: 1px;\n  transform: translateX(0);\n  animation: ${fadeInBottom} 0.5s linear forwards;\n  animation-delay: 4s;\n  transition: .4s ease-in-out;\n`","/mnt/c/Users/myron/desktop/developer/portfolio/src/pages/about.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/pages/contact.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/pages/projects.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/navbarLinks.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/feature/index.js",["172"],"import React from 'react';\nimport {\n  Container,\n  Title,\n  Subtitle,\n  Text,\n  Group,\n  Link,\n  Image,\n  Square\n} from './styles/feature'\n\nexport default function Feature({children, ...restProps}) {\n  return <Container {...restProps}>{children}</Container>\n}\n\nFeature.Group = function FeatureGroup({children, ...restProps}) {\n  return <Group {...restProps}>{children}</Group>\n}\n\nFeature.Title = function FeatureTitle({children, ...restProps}) {\n  return <Title {...restProps}>{children}</Title>\n}\n\nFeature.Subtitle = function FeatureSubtitle({children, ...restProps}) {\n  return <Subtitle {...restProps}>{children}</Subtitle>\n}\nFeature.Text = function FeatureText({children, ...restProps}) {\n  return <Text {...restProps}>{children}</Text>\n}\n\n\nFeature.Link = function FeatureLink({children, ...restProps}) {\n  return <Link {...restProps}>{children}</Link>\n}\n\nFeature.Image = function FeatureImage({...restProps}) {\n  return <Image {...restProps} />\n}","/mnt/c/Users/myron/desktop/developer/portfolio/src/components/feature/styles/feature.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/containers/feature.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/load/index.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/load/styles/load.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/socials/index.js",["173"],"import React, { useEffect, useRef } from 'react';\nimport {\n  Container,\n  Item, \n  Link, \n  List\n} from './styles/socials'\n\nlet ClickOutside = handler => {\n  let domNode = useRef()\n\n  useEffect(() => {\n    let checkHandler = event => {\n      if(!domNode.current.contains(event.target)) {\n        handler()\n      }\n    }\n    \n    document.addEventListener('mousedown', checkHandler)\n    return () => document.removeEventListener('mousedown',checkHandler)\n  }, [])\n\n  return domNode\n}\n\n\n\nexport default function Socials({children, setModalShow, ...restProps}) {\n\n  let domNode = ClickOutside(() =>{\n\n    setModalShow(false)\n  }) \n\n  return <Container ref={domNode} {...restProps}>{children}</Container>\n}\n\nSocials.List = function SocialsList({children, ...restProps}) {\n  return <List {...restProps}>{children}</List>\n}\n\nSocials.Item = function SocialsItem({children, ...restProps}) {\n  return <Item {...restProps}>{children}</Item>\n}\n\nSocials.Link = function SocialsLink({children, ...restProps}) {\n  return <Link {...restProps}>{children}</Link>\n}\n","/mnt/c/Users/myron/desktop/developer/portfolio/src/components/socials/styles/socials.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/shapes/index.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/shapes/styles/shapes.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/containers/project.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/project/index.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/project/styles/project.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/projectsData.js",[],"/mnt/c/Users/myron/desktop/developer/portfolio/src/components/form/index.js",["174","175"],"import React, { useRef, useEffect } from 'react';\nimport { \n  Container,\n  Title,\n  Input,\n  InputBox,\n  TextArea,\n  Frame,\n  Button,\n  Label\n} from './styles/form'\n\nexport default function Form({children, ...restProps}) {\n\n  return <Container {...restProps}>{children}</Container>\n}\nForm.Title = function FormTitle({children, ...restProps}) {\n  return <Title{...restProps}>{children}</Title>\n}\n\nForm.Frame = function FormFrame({children, ...restProps}) {\n  return <Frame {...restProps}>{children}</Frame>\n}\nForm.InputBox = function FormInputBox({children, ...restProps}) {\n  return <InputBox {...restProps}>{children}</InputBox>\n}\nForm.Input = function FormInput({...restProps}) {\n  return <Input {...restProps} />\n}\nForm.TextArea = function FormTextArea({...restProps}) {\n  return <TextArea